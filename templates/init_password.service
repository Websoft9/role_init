[Unit]
Description=Init Password for image
After=network.target mysql.service mysqld.service mongod.service docker.service {{init_service_before}}.service

[Service]
Type=oneshot
TimeoutSec=5s
ExecStart=/usr/bin/bash /tmp/init.sh
ExecStart=/usr/bin/systemctl disable init-password

[Install]
WantedBy=multi-user.target
